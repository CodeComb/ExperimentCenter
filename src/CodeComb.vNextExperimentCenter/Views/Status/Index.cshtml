@{ 
    ViewBag.Title = "运行状态";
}

<div class="container">
    <div class="cont-wrap">
        <div class="grid_9">
            <table class="table">
                <col style="width:15%" />
                <col style="width:7%" />
                <col style="width:7%" />
                <col style="width:7%" />
                <col />
                <col style="width:10%" />
                <col style="width:20%" />
                <thead>
                    <tr>
                        <th>Flag</th>
                        <th>得分</th>
                        <th>(ms)</th>
                        <th>(MiB)</th>
                        <th>题目</th>
                        <th>提交者</th>
                        <th>提交时间</th>
                    </tr>
                </thead>
                <tbody class="lst-statuses"></tbody>
            </table>
            @Html.MakePager()
        </div>
        <div class="grid_3 sidebar">
            <h2 class="header">检索</h2>
            <form method="get" asp-action="Index" asp-controller="Status"></form>
        </div>
    </div>
</div>