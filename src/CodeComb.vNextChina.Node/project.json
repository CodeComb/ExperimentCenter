      }
    },
    "dnxcore50": {
      "dependencies": {
        "System.IO": "4.0.11-*",
        "System.IO.FileSystem": "4.0.1-*",
        "System.IO.Compression": "4.0.1-*",
        "System.Net.Http": "4.0.1-*"
      }
